parameters:
    level: max
    paths:
        - %rootDir%/src/

    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false

    excludes_analyse:
        # Makes PHPStan crash
        - 'src/DependencyInjection/Configuration.php'
        - 'src/DependencyInjection/MonsieurBizSyliusSearchExtension.php'

        # Test dependencies
        - 'tests/Application/**/*'

        # Generated files
        - 'generated/**/*'
